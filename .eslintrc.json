{
    "extends": ["airbnb", "prettier", "react-app", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "no-var": false,
        "prettier/prettier": ["error"],
        "jsx-a11y/href-no-hash": [0],
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }]
    }
}